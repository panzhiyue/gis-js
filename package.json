{
  "name": "root",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "lerna run build",
    "build:docs": "concurrently \"lerna run build:docs\" \" vuepress build docs \"",
    "dev:docs": "concurrently \"cd packages/vue2ol && npm run dev:docs\" \"cd packages/vue2ol-extend && npm run dev:docs\" \" vuepress dev docs \"",
    "version":"lerna version",
    "publish":"lerna publish"
  },
  "devDependencies": {
    "concurrently": "^7.0.0",
    "lerna": "^4.0.0",
    "rollup": "^2.58.0",
    "rollup-plugin-less": "^1.1.3",
    "rollup-plugin-node-externals": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-vue": "^5.1.9",
    "vue-docgen-cli": "^4.45.0",
    "vue-template-compiler": "2.6.14",
    "vuepress": "^1.8.2",
    "vuepress-plugin-demo-container-v2": "^2.3.4",
    "vuepress-plugin-one-click-copy": "^1.0.2",
    "vuepress-theme-vdoing": "^1.9.5"
  }
}
