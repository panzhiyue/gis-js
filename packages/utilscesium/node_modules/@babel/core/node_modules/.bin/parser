#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/i/vue2ol/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/i/vue2ol/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../registry.npmmirror.com+@babel+parser@7.18.11/node_modules/@babel/parser/bin/babel-parser.js" "$@"
else
  exec node  "$basedir/../../../../../../registry.npmmirror.com+@babel+parser@7.18.11/node_modules/@babel/parser/bin/babel-parser.js" "$@"
fi
