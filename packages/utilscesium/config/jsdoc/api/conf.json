{
  "opts": {
    "recurse": true,
    "template": "config/jsdoc/api/template/utilscesium"
  },
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)_",
    "include": [
      "src"
    ]
  },
  "plugins": [
    "plugins/markdown"
  ],
  "markdown": {
    "parser": "gfm"
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true,
    "default": {
      "outputSourceFiles": false
    },
    "applicationName": "utilscesium"
  },
  "jsVersion": 180
}
